/* #Custom Styles
================================================== */

/* Landing Page  */

.featured-content-section {
  h2.title {
    @media only screen and (max-width: 767px) {
      font-size: 16px;
    }
  }
}

.featured-promotions-section {
  .one-third {
    img {
      width: 200px;
      height: auto;
    }
  }
}

.logo-bar-section {
  .logo-bar {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    .logo-bar__item {
      flex: 0 1 45%;
      @media only screen and (min-width: 767px){
        flex: 0 1 18%;
      }
    }
  }
}

/* Design Page */
.flex-box {
  display: flex;
  .flex-box__item {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 0 1 100%;
    @media only screen and (min-width: 767px) {
      flex: 0 1 50%;
    }
  }
  .flex-box__content {
    max-width: 600px;
    widht: 100%;
    margin: 0 auto;
  }
}
