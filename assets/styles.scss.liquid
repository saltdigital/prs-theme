      transform: translateY(0);
      -webkit-transform: translateY(0);
      -ms-transform: translateY(0);
      padding: 2em 0;
    }
  }
  .collection_title {
    font-size: 2vw;
    margin-bottom: 0.3em;
    display: inline-block;
    color: {{ settings.headline_color }};
    @include respond-to('medium') {
      font-size: inherit;
    }
  }
  .description {
    position: relative;
    font-size: 1.6vw;
